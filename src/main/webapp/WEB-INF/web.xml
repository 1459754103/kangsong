<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>TextServlet</servlet-name>
    <servlet-class>com.hsd.ner.TextServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TextServlet</servlet-name>
    <url-pattern>/cs.do</url-pattern>
  </servlet-mapping>
<servlet>
  <servlet-name>UserServlet</servlet-name>
  <servlet-class>com.hsd.servlet.UserServlet</servlet-class>
</servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/us.do</url-pattern>
  </servlet-mapping>
</web-app>

